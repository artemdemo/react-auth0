{
    "presets": [
        ["env", {
            "targets": {
                "browsers": ["last 2 versions"]
            }
        }],
        "react"
    ],
    "plugins": [
        "transform-react-jsx",
        "transform-object-assign",
        "transform-class-properties",
        "syntax-dynamic-import",
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }]
    ]

}
